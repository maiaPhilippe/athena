query ($number_of_repos: Int!, $next: String, $slug: String!, $org: String!) {
  organization(login: $org) {
    team(slug: $slug) {
      name
      teamId:id
      repositories(first: $number_of_repos, after: $next) {
        edges {
          node {
            repoId:id
            name
          }
        }
        pageInfo {
          endCursor
          hasNextPage
        }
      }
    }
  }
  rateLimit {
    remaining
    resetAt
  }
}
