query ($number_of_repos: Int!, $next: String, $slug: String!, $org: String!) {
  organization(login: $org) {
    team(slug: $slug) {
      name
      teamId:id
      members(first: $number_of_repos, after: $next) {
        edges {
          node {
            name
            memberId:id
          }
        }
        pageInfo {
          hasNextPage
          endCursor
        }
      }
    }
  }
  rateLimit {
    remaining
    resetAt
  }
}
